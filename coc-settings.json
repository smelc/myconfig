{
  "languageserver": {
    "haskell": {
      "command": "/home/churlin/.config/Code/User/globalStorage/haskell.haskell/haskell-language-server-wrapper-0.5.1-linux",
      "args": [
        "--lsp"
      ],
      "rootPatterns": [
        "hie.yaml",
        ".stack.yaml",
        "stack.yaml",
        ".hie-bios",
        "BUILD.bazel",
        "cabal.config",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "settings": {
        "languageServerHaskell": {
          "hlintOn": true,
          "maxNumberOfProblems": 50,
          "completionSnippetsOn": true
        }
      }
    },
    "ocaml-lsp": {
    "command": "opam",
    "args": ["config", "exec", "--", "ocamllsp"],
    "filetypes": ["ocaml", "reason"]
  }
  }
}
