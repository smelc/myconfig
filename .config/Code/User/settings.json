{
  "keyboard.dispatch": "keyCode",
  "editor.minimap.enabled": false,
  "hoogle-vscode.maxResults": 20,
  "reason.format.width": 0,
  "editor.rulers": [
    80,
    110
  ],
  "workbench.editor.enablePreview": false,
  "files.exclude": {
    "**/_opam": true,
    "**/.stack-work": true,
    "**/*.swp": true
  },
  "languageServerHaskell.hieVariant": "haskell-language-server",
  "[haskell]": {
    "editor.defaultFormatter": "sjurmillidahl.ormolu-vscode"
  },
  "shellcheck.disableVersionCheck": true,
  "workbench.activityBar.visible": true,
  "window.zoomLevel": 0,
  "[ocaml]": {
    "editor.defaultFormatter": "scen.ocamlformat-vscode"
  },
  "haskell.trace.server": "messages"
}
