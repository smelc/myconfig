{
  "keyboard.dispatch": "keyCode",
  "editor.minimap.enabled": false,
  "hoogle-vscode.maxResults": 20,
  "reason.format.width": 0,
  "editor.rulers": [
    80,
    110
  ],
  "workbench.editor.enablePreview": false,
  "files.exclude": {
    "**/_opam": true,
    "**/.stack-work": true,
    "**/*.dyn_hi": true,
    "**/*.dyn_o": true,
    "**/*.hi": true,
    "**/*.js_hi": true,
    "**/*.js_o": true,
    "**/*.o": true,
    "**/*.swp": true
  },
  "languageServerHaskell.hieVariant": "haskell-language-server",
  "[haskell]": {
    "editor.defaultFormatter": "sjurmillidahl.ormolu-vscode"
  },
  "shellcheck.disableVersionCheck": true,
  "[ocaml]": {
    "editor.defaultFormatter": "ocamllabs.ocaml-platform"
  },
  "haskell.trace.server": "messages",
  "diffEditor.renderSideBySide": true,
  "ocaml.trace.server": "messages",
  "haskell.updateBehavior": "prompt",
  "haskell.liquidOn": true,
  "liveshare.publishWorkspaceInfo": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "java.configuration.checkProjectSettingsExclusions": false,
  "python.languageServer": "Pylance",
  "python.showStartPage": false,
  "liveshare.authenticationProvider": "GitHub",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "haskell.hlintOn": false,
  "workbench.colorTheme": "One Dark Pro",
  "haskell.plugin.hlint.diagnosticsOn": false,
  "editor.renderIndentGuides": false,
  "editor.tabSize": 8,
  "editor.cursorStyle": "line",
  "editor.insertSpaces": false,
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
}
